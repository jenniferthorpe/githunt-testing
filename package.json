{
  "name": "githunt-react",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "axios": "^0.19.1",
    "bootstrap": "^4.4.1",
    "chai": "^4.2.0",
    "classnames": "^2.2.6",
    "core-js-bundle": "^3.6.4",
    "font-awesome": "^4.7.0",
    "mockdate": "^2.0.5",
    "moment": "^2.29.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "reactstrap": "^8.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-persist-expire": "^1.1.0",
    "redux-thunk": "^2.3.0",
    "sinon": "^8.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-chrome": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build-web": "cross-env PUBLIC_URL=/githunt react-scripts build",
    "test": "mocha --require @babel/register --require ignore-styles --require ./test/helpers.js --require mocha-snapshots 'src/**/*.spec.js'",
    "coverage": "nyc --reporter html --reporter text npm test",
    "eject": "react-scripts eject",
    "gh-pages": "NODE_DEBUG=gh-pages gh-pages -d build"
  },
  "nyc": {
    "include": "src",
    "exclude": "src/**/*.spec.js"
  },
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "axios-mock-adapter": "^1.17.0",
    "cross-env": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gh-pages": "^2.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "15.2.1",
    "jsdom-global": "3.0.2",
    "mocha": "^7.0.0",
    "mocha-snapshots": "^4.2.0",
    "nyc": "^15.0.0",
    "redux-mock-store": "^1.5.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "index.js",
  "repository": "https://github.com/jenniferthorpe/githunt-testing.git",
  "author": "Jennifer Thorpe <jennifer.thorpe@hotmail.com>",
  "license": "MIT"
}
